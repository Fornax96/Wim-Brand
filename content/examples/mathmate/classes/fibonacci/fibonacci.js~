var a = 0;
var b = 1;

function fibonacci(){
	limit = document.getElementById("limit").value;
	output = "";
	output = output + a + "<br>";
	output = output + b + "<br>";
	while(true){
		c = a + b;
		if(c > limit){
			return false
		}
		output = output + c + "<br>";
		document.getElementById("getal").innerHTML = output
		a = b;
		b = c;
	}
}
